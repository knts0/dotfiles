"""""""""""""""""""""""""""""""""
""" プラグイン
"""""""""""""""""""""""""""""""""
set nocompatible
filetype off
set rtp+=~/.vim/Vundle.vim/
call vundle#begin()

Plugin 'VundleVim/Vundle.vim'

" 導入したいプラグインを以下に列挙
" Plugin '[Github Author]/[Github repo]' の形式で記入
Plugin 'terryma/vim-expand-region'

call vundle#end()


"""""""""""""""""""""""""""""""""
""" ベース設定の読み込み
"""""""""""""""""""""""""""""""""
source $HOME/dotfiles/.basevimrc


"""""""""""""""""""""""""""""""""
""" Ideavim用のキーマップ設定
"""""""""""""""""""""""""""""""""
source $HOME/dotfiles/.ideavimrc.keymap
